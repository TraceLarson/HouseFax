{
  "name": "housefax",
  "version": "1.0.0",
  "description": "HouseFax web application for Capstone Project at Full Sail University",
  "main": "app.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node app.js",
    "server": "nodemon app.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run client \" \"npm run server\" ",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TraceLarson/HouseFax.git"
  },
  "author": "Trace Larson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TraceLarson/HouseFax/issues"
  },
  "homepage": "https://github.com/TraceLarson/HouseFax#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.3.4",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "validator": "^10.8.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.4"
  }
}
